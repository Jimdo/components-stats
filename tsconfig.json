{
  "compilerOptions": {
    "target": "es5",                          
    "module": "commonjs",                    
    "lib": ["esnext"],                     
    "allowJs": true,
    "outDir": "build",                          
    "strict": true,         
    "noImplicitAny": true,
    "esModuleInterop": true,
    "resolveJsonModule": true
  },
  "exclude": [
    "build",
    "repositories"
  ],
}
